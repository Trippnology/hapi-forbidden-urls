{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"formatter": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"complexity": {
				"noForEach": "off"
			},
			"style": {
				"noParameterAssign": "off",
				"useDefaultParameterLast": "off"
			}
		}
	},
	"organizeImports": {
		"enabled": true
	}
}
